#ifndef CODE_FUZZYPID_H_
#define CODE_FUZZYPID_H_

#include "common.h" //数据类型声明

uint8 FuzzyArray[7]={-3,-2,-1,0,1,2,3};//对数据模糊化的数组{NB,NM,NS,ZO,PS,PM,PB}

//隶属值的单位值：FuzzyArray[]*这个=隶属值
#define BiasMembership 60 //偏差
#define BiasCMembership 10//偏差于上一次偏差的差
#define SteerKP 1
#define SteerKd 1
#define MotorKP 1
#define MotorKI 1


